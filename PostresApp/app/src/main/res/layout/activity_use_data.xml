<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#F4DE79"
    tools:context=".UseData">

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginBottom="53dp"
        android:text="DATOS DE USUARIO"
        android:textSize="40dp"
        app:layout_constraintBottom_toTopOf="@+id/txt_data_id"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btn_volver_dashboard1" />

    <TextView
        android:id="@+id/txt_data_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="29dp"
        android:hint="Identificacion"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/txt_data_nombre"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <TextView
        android:id="@+id/txt_data_nombre"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="19dp"
        android:layout_marginBottom="21dp"
        android:hint="Nombre"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/txt_data_apellido"
        app:layout_constraintStart_toStartOf="@+id/txt_data_id"
        app:layout_constraintTop_toBottomOf="@+id/txt_data_id" />

    <TextView
        android:id="@+id/txt_data_apellido"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="25dp"
        android:hint="Apellido"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/txt_data_correo"
        app:layout_constraintStart_toStartOf="@+id/txt_data_nombre"
        app:layout_constraintTop_toBottomOf="@+id/txt_data_nombre" />

    <TextView
        android:id="@+id/txt_data_correo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="7dp"
        android:layout_marginBottom="21dp"
        android:hint="Correo"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/txt_data_telefono"
        app:layout_constraintStart_toStartOf="@+id/txt_data_telefono"
        app:layout_constraintTop_toBottomOf="@+id/txt_data_apellido" />

    <TextView
        android:id="@+id/txt_data_telefono"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="44dp"
        android:hint="Telefono"
        android:textSize="24sp"
        app:layout_constraintBottom_toTopOf="@+id/input_buscar_id"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_data_correo" />

    <EditText
        android:id="@+id/input_buscar_id"
        android:layout_width="219dp"
        android:layout_height="0dp"
        android:layout_marginBottom="35dp"
        android:ems="10"
        android:hint="Identificacion"
        android:inputType="text"
        app:layout_constraintBottom_toTopOf="@+id/btnBuscarUsuario"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txt_data_telefono" />

    <Button
        android:id="@+id/btnBuscarUsuario"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="60dp"
        android:backgroundTint="#F291BF"
        android:text="Button"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/input_buscar_id" />

    <Button
        android:id="@+id/btn_volver_dashboard1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="45dp"
        android:layout_marginEnd="38dp"
        android:layout_marginBottom="36dp"
        android:backgroundTint="#F291BF"
        android:text="Volver"
        app:layout_constraintBottom_toTopOf="@+id/textView5"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>